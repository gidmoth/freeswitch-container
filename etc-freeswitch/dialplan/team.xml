<include>
  <context name="team">

    <extension name="enable_srtp" continue="true">
      <condition>
        <action application="set" data="sip_secure_media=true"/>
      </condition>
    </extension>

    <extension name="teamusers_extension">
      <condition field="destination_number" expression="^(20[0-9][0-9][0-9])$">
        <action application="export" data="dialed_extension=$1"/>
        <action application="set" data="hangup_after_bridge=true"/>
        <action application="bridge" data="user/${dialed_extension}@${domain_name}"/>
      </condition>
    </extension>

    <extension name="video_uhb">
      <condition field="destination_number" expression="^30000$">
        <action application="answer"/>
        <action application="conference" data="video_uhb@v48"/>
      </condition>
    </extension>

  </context>
</include>
